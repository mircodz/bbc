namespace examples.catalog

struct Product
{
    0: required string id;
    // Breaking change: renamed required field 'name' -> 'title'
    1: required string title;
    2: optional double price;
    // Compatible addition: new optional description
    3: optional string description;
}

struct GetProductRequest
{
    0: required string id;
}

service Catalog
{
    Product get_product(GetProductRequest request);
}
